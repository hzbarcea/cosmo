<?xml version="1.0" encoding="utf-8" ?>
  <C:calendar-query xmlns:C="urn:ietf:params:xml:ns:caldav">
    <D:prop xmlns:D="DAV:">
       <D:getetag/>
       <C:calendar-data/>
    </D:prop>
     <C:filter>
          <C:comp-filter name="VCALENDAR">
                 <C:comp-filter name="VEVENT">
                      <C:prop-filter name="SUMMARY">
                        <C:text-match>event 3</C:text-match> 
                      </C:prop-filter>
           </C:comp-filter>
    </C:comp-filter>
    </C:filter>
</C:calendar-query>
